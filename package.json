{
  "name": "sportorezka",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/gavrya/sportorezka.git",
  "author": "gavrya <gavrya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon ./bin/www",
    "test": "npm run test:lint-server",
    "test:lint-server": "eslint . --ext .js",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down",
    "db:migrate": "cross-env NODE_ENV=development knex migrate:latest --verbose",
    "db:rollback": "cross-env NODE_ENV=development knex migrate:rollback --verbose",
    "db:migration": "cross-env NODE_ENV=development knex migrate:make `read name && echo $name`"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm t"
    }
  },
  "dependencies": {
    "bookshelf": "^0.14.2",
    "bookshelf-camelcase": "^2.0.1",
    "config": "^3.1.0",
    "graphql": "^14.3.1",
    "knex": "^0.16.5",
    "koa": "^2.7.0",
    "koa-graphql": "^0.8.0",
    "koa-helmet": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^2.4.0"
  }
}
